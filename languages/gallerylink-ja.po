msgid "first page"
msgstr "最初のページ"

msgid "previous page"
msgstr "前のページ"

msgid "prev"
msgstr "前のページ"

msgid "next"
msgstr "次のページ"

msgid "New"
msgstr "新着順"

msgid "Old"
msgstr "古い順"

msgid "Des"
msgstr "降順"

